FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
 volScalarFieldValue alpha.air 1.0
 volScalarFieldValue alpha.biomass 0.0
 volVectorFieldValue U (0 0 0)
);
regions
(
boxToCell
{
   box (-1 0.0 -1) (1 2.0 1);
   fieldValues
     (
      volScalarFieldValue alpha.air 0
      volScalarFieldValue alpha.biomass 1
     );
}
);

