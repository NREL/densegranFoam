FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../parameters"

phases (particles air);

particles
{
    diameterModel constant;
    constantCoeffs
    {
        d               $dS;
    }

    alphaMax        0.55;
    residualAlpha   1e-6;
}

air
{
    diameterModel constant;
    constantCoeffs
    {
        d               1;
    }

    residualAlpha   0;
}

blending
{
    default
    {
        type            none;
        continuousPhase air;
    }
}

sigma
(
    (particles and air) 0
);

aspectRatio
(
);

drag
(
    (particles in air)
    {
      type            GidaspowSchillerNaumann;//SyamlalOBrien;//Lain;//GidaspowSchillerNaumann;//Gibilaro;//Ergun;//SchillerNaumann;//GidaspowErgunWenYu;
      residualRe      1e-3;
      swarmCorrection
        {
	  type        none;
        }
    }
 );

virtualMass
(
    (particles in air)
    {
        type            constantCoefficient;
        Cvm             0.5;
    }
);

heatTransfer
(
    (particles in air)
    {
        type            RanzMarshall;
        residualAlpha   1e-3;
    }
);

lift
(
);

wallLubrication
(
);

turbulentDispersion
(
);

// Minimum allowable pressure
pMin            $p0;


