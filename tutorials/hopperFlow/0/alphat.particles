FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;


boundaryField
{
  walls
    {
        type            calculated;
        value           $internalField;
    }
  top
    {
        type            calculated;
        value           $internalField;
    }
  orifice
   {
        type            calculated;
        value           $internalField;
    }
  defaultFaces
    {
          type            empty;
    }

}


