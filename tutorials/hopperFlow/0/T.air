FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 0 0 1 0 0 0];

internalField       uniform 300;

boundaryField
{
  walls
    {
        type               zeroGradient;
    }
  top
    {
        type               inletOutlet;
        phi                phi.air;
        inletValue         uniform 300;
	value              uniform 300;
    }
  orifice
   {
        type               inletOutlet;
        phi                phi.air;
        inletValue         uniform 300;
	value              uniform 300;
   }
  defaultFaces
    {
          type             empty;
    }

}


