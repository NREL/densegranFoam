  scalar angD = 40;
  scalar AB = 1.220; // Ramp length in m
  scalar AC = 0.570; // Box length in m
  scalar AF = 0.915; // Box height in m


  scalar BC = AB+AC;
  scalar ang = degToRad(angD);
  scalar CDp = BC*sin(ang); // Elevation of left end point in mm

  scalar x3 = -BC*cos(ang);
  scalar y3 = CDp;
  scalar x9 = x3+AF*sin(ang);
  scalar y9 = y3+AF*cos(ang);
  scalar x10 = AF*sin(ang);
  scalar y10 = AF*cos(ang);