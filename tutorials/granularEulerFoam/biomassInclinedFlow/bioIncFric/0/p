FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../parameters";
dimensions          [1 -1 -2 0 0 0 0];

internalField   uniform $pR;

boundaryField
{
    left
    {
     	type               calculated;
	value              $internalField;
    }
    ramp
    {
     	type               calculated;
	value              $internalField;
    }
    top
    {
     	type               calculated;
	value              $internalField;
    }
    right
    {
     	type               calculated;
	value              $internalField;
    }
    bottom
    {
     	type               calculated;
	value              $internalField;
    }
    defaultFaces
    {
        type            empty;
    }
}



